{
  "name": "@kbthedeveloper/neumorph-player",
  "version": "0.1.6",
  "description": "Neumorph Player",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "Konstantin Bogdanov",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "rootDir": "./",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.history/",
      "<rootDir>/.next/"
    ],
    "transform": {
      "^.+\\.tsx?$": "esbuild-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "demo": "webpack serve --config=configs/webpack/dev.js --stats-error-details",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prepare": "yarn build",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "yarn test && yarn prettier && yarn lint",
    "test": "node --experimental-vm-modules --trace-warnings node_modules/jest/bin/jest.js --detectOpenHandles --no-cache"
  },
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "esbuild": "^0.15.13",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "jest": "^29.3.0",
    "jest-environment-jsdom": "^29.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.36.1",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@hot-loader/react-dom": "^17.0.2",
    "@mdi/js": "^5.9.55",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/howler": "^2.2.2",
    "@types/node": "^16.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-minify": "^0.5.2",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "howler": "^2.2.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^9.0.0",
    "path-to-regexp": "^6.2.0",
    "prettier": "^3.2.5",
    "slash": "^4.0.0",
    "styled-components": "^5.3.6",
    "tslint": "^6.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KBTheDeveloper/neumorph-player.git"
  },
  "keywords": [
    "react",
    "player",
    "neumorph",
    "audio",
    "webaudio",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/KBTheDeveloper/neumorph-player/issues"
  },
  "homepage": "https://github.com/KBTheDeveloper/neumorph-player#readme"
}
